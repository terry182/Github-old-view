{

  "description": "Swap the sidebar and news section in Github",
  "manifest_version": 2,
  "name": "Github-old-view",
  "version": "1.0",
  "homepage_url": "https://github.com/terry182/Github-old-view",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://github.com/", "*://github.com/dashboard/*"],
      "js": ["github-old-view.js"]
    }
  ]

}
